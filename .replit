modules = ["python-3.11"]

[nix]
channel = "stable-25_05"
packages = ["cargo", "libiconv", "rustc"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "web_server"

[[workflows.workflow]]
name = "web_server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd /home/runner/workspace/batman/batman/EAGLE && python web_app.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 39139
externalPort = 3000
